{
  "name": "fullstack-lem",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"node ./bin/www\" \"cd client && npm run build\"",
    "prod": "concurrently \"set NODE_ENV=production&&node ./bin/www\" \"cd client && npm run build\"",
    "dev": "concurrently \"set NODE_ENV=development&&nodemon ./bin/www\" \"cd client && npm run serve\"",
    "server": "nodemon ./bin/www",
    "client": "cd client && npm run serve"
  },
  "dependencies": {
    "concurrently": "^6.2.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  }
}
